<nav>
  <div class="nav nav-tabs" id="nav-tab" role="tablist">
    <% @versions.each_with_index do |version, index|  %>
      <%= render "versions/version_tab", index: index %>
    <% end %>
  </div>
</nav>

<div class="tab-content" id="nav-tabContent">
  <% @versions.each_with_index do |version, index|  %>
    <%= render partial: "versions/version_accordion", locals: { version: version, index: index } %>
  <% end %>
</div>


