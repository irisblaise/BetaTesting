<div class=banner2>
  <h1 class="h1-header m-2">Hey <%= current_user.first_name&.capitalize %></h1>
  <h2> These are all your versions</h2>
</div>
<div id="version-list" >
  <% @versions.each do |version|%>
  <div class="row d-flex justify-content-center w-150">
    <p class="m-3 text-center"><%= version.name %></p>
    <p class="m-3 btn btn-danger button-delete"><%= link_to "Edit version", edit_startup_version_path(version.startup_id, version.id)%></p>
    <p class="m-3 btn btn-danger button-delete"><%= link_to "Destroy version", startup_version_path(version.startup_id, version.id), method: :delete %></p>
  </div>
  <% end %>
</div>
